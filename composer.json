{
	"name": "buepro/typo3-grevman",
	"type": "typo3-cms-extension",
	"description": "Typically used to manage group events in sport clubs. Members can easily register and unregister for events and trainers might notify participants by email. Provides a matrix- list and detail view from events.",
	"keywords": [
		"TYPO3 CMS",
		"event",
		"sport",
		"club"
	],
	"homepage": "https://github.com/buepro/typo3-grevman",
	"authors": [
		{
			"name": "Roman B\u00fcchler",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^10.4"
	},
	"require-dev": {
		"typo3/testing-framework": "^6.8"
	},
	"autoload": {
		"psr-4": {
			"Buepro\\Grevman\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Buepro\\Grevman\\Tests\\": "Tests"
		}
	},
	"replace": {
		"buepro/typo3-grevman": "self.version",
		"typo3-ter/grevman": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/public",
			"extension-key": "grevman"
		},
		"branch-alias": {
			"dev-master": "0.x-dev"
		}
	}
}
